<html lang="en">
  <head>
    <title>AngularJS Seed App | LiftOff LLC</title>
    <link rel="stylesheet" href="public/styles/bootstrap.min.css">

    {+more_head_opts}
    {/more_head_opts}    
  </head>

  <body>

    <div class="container" ng-app="donorApp" ng-controller="BaseCtrl">

      <div class="masthead">
        <h3 class="muted">Donation Signup</h3>
        <div class="navbar">
          <div class="navbar-inner">
            <div class="container">
              <ul class="nav">
                <li class="active" lo-titles="{'tab': 'home', 'title': 'Home'}">
                  <a href="/dashboard#/home">Home</a>
                </li>
                <li lo-titles="{'tab': 'all', 'title': 'Users'}">
                  <a href="/dashboard#/all">Users</a>
                </li>
                <li lo-titles="{'tab': 'visitor', 'title': 'Visitors'}">
                  <a href="/dashboard#/visitor">Visitors</a>
                </li>
                <li><a href="/livechat">Live Chat</a></li>
                <li><a href="#">About</a></li>
                <li><a href="#">Contact</a></li>
              </ul>
            </div>
          </div>
        </div><!-- /.navbar -->
      </div>

      {+content}
      {/content}

      <hr>

      <div class="footer">
        <p>&copy; Company 2013</p>
      </div>

    </div> <!-- /container -->

  <script src="http://code.jquery.com/jquery-1.9.1.min.js" type="text/javascript"></script>
  <script src="/public/js/angular/scripts/vendor/angular.min.js" type="text/javascript"></script>
  <script src="/public/js/angular/scripts/vendor/angular-resource.js" type="text/javascript"></script>
  <script src="/public/js/angular/scripts/vendor/bootstrap.min.js" type="text/javascript"></script>

  <!-- main app JS, that ties things together -->
  <script src="/public/js/angular/scripts/donor.js" type="text/javascript"></script>

  <!-- include the controllers here -->
  <script src="/public/js/angular/scripts/controllers/home.js" type="text/javascript"></script>
  <script src="/public/js/angular/scripts/controllers/base.js" type="text/javascript"></script>
  <script src="/public/js/angular/scripts/controllers/user.js" type="text/javascript"></script>
  <script src="/public/js/angular/scripts/controllers/visitor.js" type="text/javascript"></script>

  <!-- include the controllers here -->
  <script src="/public/js/angular/scripts/services/user.js" type="text/javascript"></script>

  <!-- your directives -->
  <script src="/public/js/angular/scripts/directives/component.js" type="text/javascript"></script>  
</body></html>